<section class="form">
  <h1>Register for Muzak</h1>
  <%= render partial: 'shared/errors' %>
  <form class="" action="<%= users_url %>" method="post">
    <%= auth_token %>
    <label id = "email" for="email">Email:</label>
    <input id = "email" type="text" name="user[email]" value="">

    <br>

    <label id = "password" for="password">Password:</label>
    <input id = "password" type="password" name="user[password]" value="">

    <br>

    <input type="submit" value="Register Muzak!">
  </form>
</section>
